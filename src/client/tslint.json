{
  "rulesDirectory": [
    "../../node_modules/codelyzer"
  ],
  "extends": [
    "angular-tslint-rules"
  ],
  "rules": {
    "object-literal-key-quotes": [
      false
    ],
    "align": [
      false
    ],
    "no-console": [
      false
    ],
    "curly": false,
    "typedef": [
      false
    ],
    "member-ordering": [
      false
    ],
    "member-access": [
      "no-public",false
    ],
    "newline-before-return": false,
    "directive-selector": [
      true,
      "attribute",
      [
        "app"
      ],
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      [
        "pm",
        "app",
        "test",
        "i18n"
      ],
      "kebab-case"
    ],
    "use-view-encapsulation": false,
    "pipe-naming": [
      true,
      "camelCase",
      "kr"
    ],
    "array-type": [
      false
    ]
  }
}