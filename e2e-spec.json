{
  "moduleFileExtensions": [
    "ts",
    "js",
    "html"
  ],
  "testRegex": "(/__tests__/.*|\\.(test|e2e-spec))\\.(ts|js)$",
  "transform": {
    "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "globals": {
    "__TRANSFORM_HTML__": true,
    "__process_env__": {},
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  }
}