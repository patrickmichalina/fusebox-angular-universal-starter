    "@types/mkdirp": "^0.3.29",
    "@types/morgan": "^1.7.32",
    "@types/node-sass": "^3.10.32",
    "@types/serve-favicon": "^2.2.28",
    "fuse-box": "2.1.0",
    "fusebox-chain-plugin": "^1.0.7",
    "glob": "^7.1.2",
    "mkdirp": "^0.5.1",
    "ng2-fused": "^0.3.3",
    "node-sass": "^4.5.3",
    "rollup": "^0.43.0",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.0",
    "uglify-es": "^3.0.20"
  },
  "dependencies": {
    "@angular/animations": "4.2.4",
    "@angular/common": "4.2.4",
