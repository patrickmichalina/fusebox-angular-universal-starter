{
  "name": "fusebox-angular-universal-starter",
  "version": "0.0.0",
  "description": "Angular Universal starter build pipeline using FuseBox",
  "license": "MIT",
  "homepage": "https://angular.patrickmichalina.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickmichalina/fusebox-angular-universal-starter"
  },
  "bugs": {
    "email": "patrickmichalina@mac.com",
    "url": "https://github.com/patrickmichalina/fusebox-angular-universal-starter/issues"
  },
  "author": {
    "email": "patrickmichalina@mac.com",
    "name": "Patrick Michalina",
    "url": "https://patrickmichalina.com"
  },
  "readme": "https://github.com/patrickmichalina/fusebox-angular-universal-starter/blob/master/README.md",
  "scripts": {
    "test": "jest",
    "test.ci": "jest --ci --updateSnapshot",
    "test.snapshots": "jest --updateSnapshot",
    "test.coverage": "jest --coverage",
    "test.watch": "jest --watch",
    "start": "ts-node fuse serve",
    "start.aot": "ts-node fuse serve --aot",
    "start.spa": "ts-node fuse serve --spa",
    "start.prod": "NODE_ENV=prod ts-node fuse serve",
    "ngc": "ts-node fuse ngc",
    "heroku-postbuild": "npm run start.prod",
    "postinstall": "npm prune && npm run webdriver.update",
    "test.e2e.start": "npm run test.e2e.ts && node_modules/.bin/ts-node node_modules/.bin/protractor tools/test/protractor.conf.ts",
    "test.e2e.ts": "node_modules/.bin/tsc -p tsconfig-e2e.json",
    "webdriver.update": "node_modules/.bin/webdriver-manager update",
    "webdriver.start": "node_modules/.bin/webdriver-manager start"
  },
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/glob": "^5.0.30",
    "@types/html-minifier": "^1.1.30",
    "@types/jest": "^20.0.2",
    "@types/js-cookie": "^2.0.28",
    "@types/jsdom": "^11.0.1",
    "@types/memory-cache": "0.0.29",
    "@types/mkdirp": "^0.3.29",
    "@types/morgan": "^1.7.32",
    "@types/node-sass": "^3.10.32",
    "@types/serve-favicon": "^2.2.28",
    "@types/string-hash": "^1.1.0",
    "@types/yargs": "^8.0.1",
    "angular-tslint-rules": "^1.0.3",
    "bootstrap": "^4.0.0-alpha.6",
    "codelyzer": "^3.1.2",
    "express-minify-html": "^0.11.4",
    "favicons": "^4.8.6",
    "fuse-box": "^2.2.0",
    "fuse-box-typechecker": "git://github.com/fuse-box/fuse-box-typechecker.git",
    "glob": "^7.1.2",
    "hash-files": "^1.1.1",
    "html-minifier": "^3.5.2",
    "jest": "^20.0.4",
    "jest-junit-reporter": "^1.1.0",
    "jest-preset-angular": "^2.0.5",
    "jsdom": "^11.1.0",
    "mkdirp": "^0.5.1",
    "ng2-fused": "^0.3.3",
    "node-sass": "^4.5.3",
    "protractor": "^5.1.2",
    "rollup": "^0.43.1",
    "sitemap-generator": "^6.0.0",
    "string-hash": "^1.1.3",
    "svg2png": "^4.1.1",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "uglify-es": "^3.0.23",
    "uglify-js": "^3.0.24",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "@angular/animations": "^4.3.0-rc.0",
    "@angular/common": "^4.3.0-rc.0",
    "@angular/compiler": "^4.3.0-rc.0",
    "@angular/compiler-cli": "^4.3.0-rc.0",
    "@angular/core": "^4.3.0-rc.0",
    "@angular/forms": "^4.3.0-rc.0",
    "@angular/http": "^4.3.0-rc.0",
    "@angular/platform-browser": "^4.3.0-rc.0",
    "@angular/platform-browser-dynamic": "^4.3.0-rc.0",
    "@angular/platform-server": "^4.3.0-rc.0",
    "@angular/router": "^4.3.0-rc.0",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "@ngx-meta/core": "^0.4.0-rc.2",
    "angular2-jwt": "^0.2.3",
    "angulartics2": "^2.2.2",
    "compressible": "^2.0.10",
    "core-js": "2.4.1",
    "detect-adblock": "0.0.2",
    "express": "^4.15.3",
    "js-cookie": "^2.1.4",
    "morgan": "^1.8.2",
    "rxjs": "^5.4.2",
    "serve-favicon": "^2.4.3",
    "shrink-ray": "^0.1.3",
    "systemjs": "^0.20.15",
    "zone.js": "0.8.12"
  },
  "engines": {
    "node": ">= 8.1.3",
    "npm": ">= 5.2.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./tools/test/jest.setup.ts",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "globals": {
      "__TRANSFORM_HTML__": true,
      "__process_env__": {},
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  }
}
