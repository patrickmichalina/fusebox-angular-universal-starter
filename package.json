    "core-js": "2.4.1",
    "express": "^4.15.3",
    "jsdom": "^11.0.0",
    "morgan": "^1.8.2",
    "normalize.css": "^7.0.0",
    "rxjs": "5.4.1",
    "serve-favicon": "^2.4.3",
    "systemjs": "^0.20.14",
    "zone.js": "0.8.12"
