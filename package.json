    "@angular/core": "5.0.0-beta.7",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "5.0.0-beta.7",
    "@angular/http": "5.0.0-beta.7",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "5.0.0-beta.7",
    "@angular/platform-browser-dynamic": "5.0.0-beta.7",
    "@angular/platform-server": "5.0.0-beta.7",
    "@angular/router": "5.0.0-beta.7",
    "@expo/bunyan": "^1.8.10",
    "@nguniversal/express-engine": "^5.0.0-beta.0",
    "@ngx-meta/core": "^4.0.1",
    "angular2-jwt": "^0.2.3",
    "angulartics2": "^2.4.0",
    "bunyan-middleware": "^0.8.0",
    "compressible": "^2.0.11",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "express": "^4.15.4",
    "getos": "^3.0.1",
    "hammerjs": "^2.0.8",
    "js-cookie": "^2.1.4",
    "markdown-to-html-pipe": "^1.2.3",
    "ms": "^2.0.0",
    "nightmare": "^2.10.0",
    "rxjs": "^5.4.3",
    "serve-favicon": "^2.4.4",
    "shrink-ray": "^0.1.3",
    "stacktrace-js": "^2.0.0",
    "systemjs": "^0.20.19",
    "tsickle": "^0.24.1",
    "url-parse": "^1.1.9",
    "zone.js": "^0.8.17"
  },
  "engines": {
    "node": ">= 8.4.0",
    "npm": ">= 5.4.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": "condition-circle",
    "getLastRelease": "last-release-git"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "moduleNameMapper": null,
    "setupTestFrameworkScriptFile": "./tools/test/jest.setup.ts",
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "collectCoverageFrom": [
      "src/client/app/**/*(*.service|*.component|*.pipe|*.directive|*.module).{ts,html}",
      "!src/client/app/shared/shared.module.ts",
      "src/server/**/**.{ts}"
    ],
    "globals": {
      "__TRANSFORM_HTML__": true,
      "__process_env__": {},
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  }
}
